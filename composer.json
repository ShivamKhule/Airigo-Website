{
    "name": "airigojobs/website",
    "description": "Aviation & Hospitality Job Portal",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "kreait/firebase-php": "^5.26"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "ext-grpc": "1.0",
            "ext-sodium": "1.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}